{
  "name": "protodef-neo",
  "version": "0.1.0",
  "description": "ProtoDef reimplementation on Node.JS",
  "module": "./src/index.js",
  "main": "./index.js",
  "scripts": {
    "start": "node -r esm ./src/index",
    "coverage": "nyc ava",
    "test": "ava",
    "bench": "cross-env NODE_ENV=benchmark ava --serial --timeout=1m",
    "lint": "standard --fix"
  },
  "repository": "github:Saiv46/node-protodef-neo",
  "keywords": [],
  "author": "Saiv46",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "standard": {
    "globals": [
      "BigInt"
    ]
  },
  "ava": {
    "verbose": true,
    "require": [
      "esm"
    ]
  },
  "nyc": {
    "instrument": true,
    "all": true
  },
  "bugs": "https://github.com/Saiv46/node-protodef-neo/issues",
  "homepage": "https://github.com/Saiv46/node-protodef-neo#readme",
  "dependencies": {
    "bit-buffer": "^0.2.4",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "ava": "^3.5.0",
    "chuhai": "^1.2.0",
    "cross-env": "^7.0.2",
    "nyc": "^15.0.0",
    "standard": "^14.3.3"
  }
}
