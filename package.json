{
  "name": "protodef-neo",
  "version": "0.1.1",
  "description": "ProtoDef reimplementation on Node.JS",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.js",
    "./datatypes": "./src/datatypes/index.js",
    "./protodef": "./src/legacy.js"
  },
  "scripts": {
    "test": "ava",
    "coverage": "nyc ava",
    "bench": "cross-env NODE_ENV=benchmark ava --serial --timeout=1m",
    "bench:compiled": "cross-env PROTODEF_COMPILE_STRUCTS=1 npm run bench",
    "lint": "standard --fix"
  },
  "repository": "github:Saiv46/node-protodef-neo",
  "keywords": [
    "protodef",
    "protocol",
    "serialization",
    "deserailization"
  ],
  "author": "Saiv46",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "ava": {
    "verbose": true,
    "files": ["tests/**/*.spec.js"]
  },
  "nyc": {
    "instrument": true,
    "all": true
  },
  "bugs": "https://github.com/Saiv46/node-protodef-neo/issues",
  "homepage": "https://github.com/Saiv46/node-protodef-neo#readme",
  "dependencies": {
    "bit-buffer": "^0.2.4"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "chuhai": "1.2.0",
    "cross-env": "7.0.3",
    "standard": "17.0.0",
    "nyc": "15.1.0"
  },
  "files": ["src/**"]
}
