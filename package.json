{
  "name": "protodef-neo",
  "version": "0.1.1",
  "description": "ProtoDef reimplementation on Node.JS",
  "module": "./src/index.mjs",
  "main": "./src/index.cjs",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./src/index.cjs"
    },
    "./datatypes": {
      "import": "./src/datatypes/index.mjs",
      "require": "./src/datatypes.cjs"
    },
    "./protodef": {
      "import": "./src/legacy.mjs",
      "require": "./src/legacy.cjs"
    }
  },
  "scripts": {
    "test": "ava",
    "coverage": "nyc ava",
    "bench": "cross-env NODE_ENV=benchmark ava --serial --timeout=1m",
    "bench:compiled": "cross-env PROTODEF_COMPILE_STRUCTS=1 npm run bench",
    "lint": "standard --fix"
  },
  "repository": "github:Saiv46/node-protodef-neo",
  "keywords": [
    "protodef",
    "protocol",
    "serialization",
    "deserailization"
  ],
  "author": "Saiv46",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "ava": {
    "verbose": true,
    "require": ["esm"],
    "files": ["tests/**/*.spec.js"]
  },
  "nyc": {
    "instrument": true,
    "all": true
  },
  "bugs": "https://github.com/Saiv46/node-protodef-neo/issues",
  "homepage": "https://github.com/Saiv46/node-protodef-neo#readme",
  "dependencies": {
    "bit-buffer": "^0.2.4",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "chuhai": "1.2.0",
    "cross-env": "7.0.3",
    "standard": "16.0.4",
    "nyc": "15.1.0"
  },
  "files": ["src/**"]
}
