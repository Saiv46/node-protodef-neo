{
  "name": "protodef-neo",
  "version": "0.1.0",
  "description": "ProtoDef reimplementation on Node.JS",
  "module": "./src/index.mjs",
  "main": "./index.cjs",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./index.cjs"
    },
    "./datatypes": {
      "import": "./src/datatypes/index.mjs",
      "require": "./datatypes.cjs"
    },
    "./protodef": {
      "import": "./src/legacy.mjs",
      "require": "./legacy.cjs"
    }
  },
  "scripts": {
    "test": "ava",
    "bench": "cross-env NODE_ENV=benchmark ava --serial --timeout=1m",
    "lint": "standard --fix"
  },
  "repository": "github:Saiv46/node-protodef-neo",
  "keywords": ["protodef", "protocol", "serialization", "deserailization"],
  "author": "Saiv46",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "ava": {
    "verbose": true,
    "require": ["esm"]
  },
  "nyc": {
    "instrument": true,
    "all": true
  },
  "bugs": "https://github.com/Saiv46/node-protodef-neo/issues",
  "homepage": "https://github.com/Saiv46/node-protodef-neo#readme",
  "dependencies": {
    "bit-buffer": "^0.2.4",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "ava": "^3.5.0",
    "chuhai": "^1.2.0",
    "cross-env": "^7.0.2",
    "standard": "^14.3.3"
  }
}